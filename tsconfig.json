      "baseUrl": ".",
      "experimentalDecorators": true,
      "declaration": false,
      "target": "esnext",
      "module": "commonjs",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "sourceMap": true,
      "noImplicitAny": true,
      "outDir": "./dist",
      "lib": [
          "es2015",
          "es2016",
          "es2017",
          "dom"
          ],
      "paths": {
          "@themost/xml": [
              "./src/index"
          ]
      }
    },
    "exclude": [
        "node_modules"
        ]
  }